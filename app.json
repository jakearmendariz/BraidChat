{
  "expo": {
    "name": "BraidChat",
    "slug": "BraidChat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/BCicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "primaryColor": "#0d4771",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.braid.BraidChat",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.braid.BraidChat",
      "versionCode": 1,
      "windowSoftInputMode":"stateUnspecified|adjustResize"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "description": "A chat app using the braidjs protocol to synchronize messages."
  }
}
