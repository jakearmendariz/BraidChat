{
  "expo": {
    "name": "BraidChat",
    "slug": "BraidChat",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/BCicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "primaryColor": "#0d4771",
    "splash": {
      "image": "./assets/images/whitesplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/images/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.braid.BraidChat",
      "buildNumber": "1.0.0"
    },
    "notification": {
      "icon": "./assets/images/notification-icon.png"
    },
    "android": {
      "package": "com.braid.BraidChat",
      "googleServicesFile": "./google-services.json",
      "versionCode": 3,
      "permissions":["NOTIFICATIONS"],
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "description": "A chat app using the braidjs protocol to synchronize messages."
  }
}
